{
  "name": "pixeld",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint . --fix",
    "tsc": "tsc",
    "prebuild": "npm run lint",
    "build": "npm-run-all clean tsc",
    "dev": "node --loader ts-node/esm src/server.ts",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "dotenv": "^17.2.4",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.2",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=24.13.1",
    "npm": ">=11.8.0"
  }
}
