{
  "name": "pixeld",
  "version": "1.0.0",
  "description": "Refactored for Jest testing",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint . --fix",
    "tsc": "tsc",
    "prebuild": "npm run lint",
    "build": "npm-run-all clean tsc",
    "dev": "nodemon --watch src -e ts,ejs --exec ts-node src/server.ts",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.4",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/dotenv": "^8.2.3",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^25.2.3",
    "@types/shelljs": "^0.10.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.39.2",
    "fs-extra": "^11.3.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.2",
    "shelljs": "^0.10.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=24.13.1",
    "npm": ">=11.8.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testMatch": [
      "<rootDir>/test/*.test.ts"
    ]
  }
}
